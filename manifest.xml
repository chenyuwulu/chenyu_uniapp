<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://www.w7.cc" versionCode="">
	<application setting='false'>
		<name><![CDATA[uniapp微擎示例]]></name>
		<identifie><![CDATA[chenyu_uniapp]]></identifie>
		<version><![CDATA[1.0.7]]></version>
		<ability><![CDATA[uniapp微擎示例]]></ability>
		<type><![CDATA[chenyu_uniapp]]></type>
		<description><![CDATA[uniapp是一个跨端的前端开发框架，详细内容自行访问https://uniapp.dcloud.io查看文档
本示例旨在帮助各开发者如何使用uniapp搭配微擎跨端开发
交流QQ群36492501]]></description>
		<author><![CDATA[尘雨雾录]]></author>
		<url><![CDATA[]]></url>
		<setting><![CDATA[]]></setting>
	</application>
	<platform>
		<rule embed="false" />
		<rule embed="false" />
	<supports>
<!--		微信小程序-->
		<item type="wxapp" />
<!--		微信公众号-->
		<item type="app" />
<!--		支付宝小程序-->
		<item type="aliapp" />
<!--		字节跳动小程序-->
		<item type="toutiaoapp" />
<!--		Android-->
		<item type="android" />
	</supports>
	</platform>
	<bindings>
		<menu>
			<entry title="微信公众号" do="app_wx" multilevel="true" icon="" displayorder="50" />
				<entry title="设置" do="app_wx_site" parent="app_wx" displayorder="1" />
			<entry title="微信小程序" do="mp_weixin" multilevel="true" icon="" displayorder="40" />
				<entry title="设置" do="mp_weixin_site" parent="mp_weixin" displayorder="2" />
				<entry title="用户管理" do="mp_weixin_user" parent="mp_weixin" displayorder="1" />
			<entry title="支付宝小程序" do="mp_alipay" multilevel="true" icon="" displayorder="30" />
				<entry title="设置" do="mp_alipay_site" parent="mp_alipay" displayorder="2" />
				<entry title="用户管理" do="mp_alipay_user" parent="mp_alipay" displayorder="1" />
			<entry title="字节跳动小程序" do="mp_byte" multilevel="true" icon="" displayorder="20" />
				<entry title="设置" do="mp_byte_site" parent="mp_byte" displayorder="2" />
				<entry title="用户管理" do="mp_byte_user" parent="mp_byte" displayorder="1" />
			<entry title="APP-Android" do="android" multilevel="true" icon="" displayorder="10" />
				<entry title="设置" do="android_site" parent="android" displayorder="2" />
				<entry title="用户管理" do="android_user" parent="android" displayorder="1" />
		</menu>
	</bindings>
	<install><![CDATA[sql/install.php]]></install>
	<uninstall><![CDATA[]]></uninstall>
	<upgrade><![CDATA[sql/upgrade.php]]></upgrade>
</manifest>